body{
    font-size: 16px;
    /*equals 1em; */
}
/*buttons */
.ml-primary-button{
    background: #e51f26;
    color: #fff;
    font-family: 'Roboto', sans-serif;
    font-weight: 500;
    font-size: 1.225em;
    border: 0px;
    border-radius: 0px;
    padding: 0.2em 1em;
}

.ml-large{
    margin-top: 2em;
    padding: 1em;
}

.ml-primary-button:hover{
    color: #fbac00;
    background: #e51f26;
}

.ml-top-pad{
    padding-top: 0px !important; 
}

.ml-primary-button i{
    font-size: 0.9em;
    margin-right: 0.25em;
}

/* bread crumbs */
.ml-breadcrumbs{
    padding: 0.5em 0em;
    float: left;
    width: 100%;
}

.ml-breadcrumbs ul{
    padding: 0px;
    margin: 0px;
}

.ml-breadcrumbs ul li{
    list-style: none;
    display: inline;
    padding-left: 0px;
    font-family: 'Roboto', sans-serif;
    font-size: 1em;
    color: #333; 
}

.ml-breadcrumbs ul li a{
    font-family: 'Roboto', sans-serif;
    font-size: 1em;
    color: #318ba9;
}


/*headers */
.ml-headings-one{
    font-family: 'Raleway', sans-serif;
    font-size: 1.875em;
    font-weight: 700;
    color: #e51f26;
    text-transform: uppercase;
    margin-top: 1.5em;
}


.ml-box-headings{
    font-family: 'Roboto', sans-serif;
    font-size: 1.675em;
    font-weight: 500;
    color: #fff;
    text-transform: uppercase;
}

/*launch pad */
.ml-launchpad{
    background-color: #e51f26;
    /*height: 3em;*/
    position: fixed;
    color: #fff;
    width: 100%;
    -webkit-box-shadow: 5px 5px 10px 0px rgba(161,156,161,1);
    -moz-box-shadow: 5px 5px 10px 0px rgba(161,156,161,1);
    box-shadow: 5px 5px 10px 0px rgba(161,156,161,1);
    z-index: 10000;
}

.ml-launchpad-list{
    margin-bottom: 0em !important;
}

.ml-launchpad-list li a{
    color: #fff;
    padding: 0.4em 1.8em;
    font-family: 'Roboto', sans-serif;
    font-size: 1.225em;
    font-weight: 400;
    cursor: pointer;
}

.ml-launchpad-list li a:hover{
    text-decoration: none;
    color: #fbac00;
}


.ml-launchpad-list .ml-launchpad-header {
    color: #e51f26;
    background: #fff;
    padding: 0.4em;
    font-family: 'Roboto', sans-serif;
    font-size: 1.375em;
    font-weight: 600;
}

.ml-launchpad-list li a i{
    margin: 0em 0.4em;
}

.ml-launchpad-dropdown{
    background: #e51f26;
    color: #fff;
}

.ml-launchpad-dropdown li a{
    color: #fff;
    font-family: 'Roboto', sans-serif;
    font-weight: 500;
    font-size: 1.225em;
    background: #e51f26;
    padding: 0.5em 0.5em;
}

.ml-launchpad-dropdown.dropdown-menu>li>a:focus,.ml-launchpad-dropdown.dropdown-menu>li>a:hover{
    background: #e51f26;
    color: #fbac00;
}

.arrow-right {
    position: absolute;
    left:155px;
    top: -2px;
  width: 0; 
  height: 0; 
  border-top: 26px solid transparent;
  border-bottom: 26px solid transparent;
  border-left: 26px solid #fff;
}

/* masthead */
.ml-masthead{
    margin-top: 4em;
    float: left;
    width:100%;
}

.ml-pa-logo-container img{
    /*width: 120px;*/
    height: 100px;
}
.right{
    text-align: right;
    float: right;
}

.ml-masthead-links li a{
     font-family: 'Roboto', sans-serif;
    font-size: 1em;
    font-weight: 400;
    color: #318ba9;
    cursor: pointer;
}

.ml-masthead-links li a:hover{
    text-decoration: none;
    color: #e51f26;
}

.ml-masthead-search{
    width: 80%;
    position: relative;
    float: right;
}

.ml-masthead-search input{
    font-family: 'Roboto', sans-serif;
    font-size: 1em;
    border: 0px;
    padding: 0.5em;
    background: #fff;
    border-bottom: #e51f26 1px solid;
    width: 100%;
}

.ml-masthead-search input:focus{
    outline: none;
    border: 0px;
    border-bottom: #318ba9 1px solid;
    width: 100%;
}

.ml-masthead-search button{
    border: 0px;
    background: transparent;
    padding: 0.5em;
    color: #e51f26;
    font-size: 1.225em;
    position: absolute;
    right: 0px;
    bottom: 0px;
}

.ml-masthead-search button:hover{
    background: transparent;
    color: #318ba9;
}


/*footer */
.ml-footer{
    background: #e51f26;
    color: #fff;
    font-family: 'Roboto', sans-serif;
    font-size: 1em;
    font-weight: 400;
    width: 100%;
    padding: 2em 0em;
    margin-top: 2em;
}

.ml-footer-list{
    margin-bottom: 0em;
    padding-left: 0em;
}

.ml-footer-list li {
    list-style: none;
    padding: 0.3em;
}

.ml-footer-list li a{
    color: #fff;
    cursor: pointer;
}

.ml-footer-list li a:hover{
    text-decoration: none;
}

.ml-copyright{
    margin-top: 6em;
    text-align: right;
}

/*app containers*/
/* stripped app containers */
.ml-app-cards-striped-container{
    width: 100%;
}

.ml-app-cards-striped{
    padding: 2.5em 1em;
    text-align: center;
    height: 120px;
    margin: 1em 0em 0em 0em;
    position: relative;
}

.ml-app-card-colors{
    padding: 2.5em 1em;
    width:100%;
    text-align: center;
    height: 150px;
    margin: 3em 0em 1em 0em;
    position: relative;
}

.ml-white-circles{
    background: #fff;
    width: 6em;
    height: 6em;
    position: absolute;
    top: -30%;
    left: 35%;
    border-radius: 50%;
    padding-top: 0.5em;
}

.ml-white-circles-striped i,.ml-white-circles-striped-narrow i, .ml-white-circles i{
    font-size: 3.5em;
    margin-top: 1.5em;
}

.ml-white-circles-striped{
    background: #fff;
    width: 6em;
    height: 6em;
    position: absolute;
    top: -40%;
    left: 45%;
    border-radius: 50%;
    padding-top: 0.5em; 
}

.ml-white-circles-striped-narrow{
    background: #fff;
    width: 6em;
    height: 6em;
    position: absolute;
    top: -40%;
    left: 35%;
    border-radius: 50%;
    padding-top: 0.5em;
}

