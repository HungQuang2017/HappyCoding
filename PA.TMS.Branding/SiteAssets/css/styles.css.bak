/* quick access */
.ml-bordered-container{
    border: 1px solid #ddd;
    padding: 1em;
    -webkit-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    -moz-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
}


.ml-quick-access{
    list-style: none;
    padding-left: 0px;
}

.ml-quick-access li{
    padding: 0.5em 0em;
}

.ml-quick-access li a{
    font-family: 'Roboto', sans-serif;
    font-size: 1.115em;
    line-height: 1.35;
    font-weight: 400;
    color: #318ba9;
    cursor: pointer;
}

.ml-quick-access li a:hover{
    text-decoration: none;
    color: #e51f26;
}


.ml-quick-access li a:before{
    content: "\f08e";
    font-family: 'FontAwesome';
    font-size: 1em;
    margin-right: 0.5em;
}

/* app containers */
.ml-app-cards-striped-container{
    -webkit-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    -moz-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
}

.ml-app-cards-striped-content{
    padding: 1em;
}

/*meerkats*/
.meerkat-left-link{
 font-family: 'Roboto', sans-serif;
    font-size: 1em;
    font-weight: 400;
    color: #318ba9;
    cursor: pointer;
    text-align: left;
}

.meerkat-right-link{
    font-family: 'Roboto', sans-serif;
    font-size: 1em;
    font-weight: 400;
    color: #318ba9;
    cursor: pointer;
    float: right;
}

.meerkat-left-link:hover, .meerkat-right-link:hover{
    color:#e51f26;
    text-decoration: none;
}

.ml-app-cards-striped-content img{
    margin-bottom: 0.75em;
}

.meerkat-container{
    position: relative;
    padding-top: 3.5em;
}

.meerkat-container img{
    width: 100%;
}


.meerkat-content{
    width: 410px;
    height: 170px;
    position: absolute;
    top: 45%;
    left: 20%;
}





.meerkat-content h4{
    font-family: "Raleway", sans-serif;
    font-size: 1.75em;
    color: #60a917;
    font-weight: 600;
}

.meerkat-content p{
    font-family: "Roboto", sans-serif;
    font-size: 1em;
    color: #333;
    font-weight: 400;
    height: 85px;
    text-overflow: ellipsis;
    overflow-y: hidden;
}

.meerkat-content h5{
    font-family: "Raleway", sans-serif;
    font-size: 1.25em;
    color: #60a917;
    font-weight: 600;
}

/* events container */

.event-container{
    width:100%;
    height: 90px;
    margin: 0.5em 0em;
    -webkit-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    -moz-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
}

.event-container p{
    margin-bottom: 5px;
    font-family: 'Roboto', sans-serif;
    font-weight: 400;
    font-size: 1em;
}

.event-time{
    width: 40%;
    float: left;
    text-align: center;
    background-color: #60a917;
    color: #fff;
    padding: 0.5em;
    height: 100%;
}

.event-time.alternate{
    background-color: #318ba9;
}

.event-caption{
    width: 60%;
    float: left;
    padding: 0.5em;
    height: 100%;
}

.left-link{
     font-family: 'Roboto', sans-serif;
    font-size: 1em;
    font-weight: 400;
    color: #318ba9;
    cursor: pointer;
    text-align: left;
}

.left-link:hover{
    text-decoration: none;
    color: #e51f26;
}

.billboard img{
    width: 215px;
    -webkit-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    -moz-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    margin: 0em 0em 1em 0em;
}

.billboard{
    margin: 0em 0em 0em 0em;
}

/* main menu bar */
.navbar-default{
    float: left;
    width: 100%;
    margin-top: 1em;
    background-color: #fff;
    border: 0px;
    border-bottom: 1px solid #ddd;
    -webkit-box-shadow: 3px 3px 6px 0px rgba(161,156,161,1);
    -moz-box-shadow: 3px 3px 6px 0px rgba(161,156,161,1);
    box-shadow: 3px 3px 6px 0px rgba(161,156,161,1);
    border-radius: 0px;
    margin-bottom: 0px; 
}


.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover{
    color: #e51f26;
    background-color: #fff;
}

.navbar-default li a{
    font-family: 'Roboto', sans-serif;
    font-weight: 600;
    font-size: 1.2em;
    color: #333;
}

.navbar-default .navbar-nav>li>a{
    color: #333;
}

.navbar-default .navbar-nav>li>a:hover{
    color: #e51f26;
}

.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus, .navbar-default .navbar-nav>.open>a:hover{
    color: #e51f26;
    background: #fff;
}

.yamm-content li h3{
