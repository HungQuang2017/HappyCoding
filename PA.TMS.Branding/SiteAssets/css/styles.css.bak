/* carousel */

.carousel-inner{
    position: relative;
    -webkit-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    -moz-box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
    box-shadow: 1px 1px 3px 2px rgba(161,156,161,1);
}

.carousel-control {
    display: none;
}

.carousel-indicators{
    position: absolute;
    top: 20px;
    left: 28%;
    width: 20%;
    height: 20%;
}

.carousel-indicators li{
    width: 20px;
    height: 20px;
    background-color: #fff;
    border-color: #fff;
}

.carousel-indicators li.active{
    width: 20px;
    height: 20px;
    background-color: #d3573f;
    border-color: #d3573f;
}

.ml-article-caption{
    position: absolute;
    left: 0px;
    bottom: 0px;
    height: 200px;
    padding: 6em 1em 1em 1em;
    width: 100%;
    background: rgba(117,113,117,0);
    background: -moz-linear-gradient(top, rgba(117,113,117,0) 0%, rgba(117,116,117,0.62) 27%, rgba(0,0,0,0.98) 100%);
    background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(117,113,117,0)), color-stop(27%, rgba(117,116,117,0.62)), color-stop(100%, rgba(0,0,0,0.98)));
    background: -webkit-linear-gradient(top, rgba(117,113,117,0) 0%, rgba(117,116,117,0.62) 27%, rgba(0,0,0,0.98) 100%);
    background: -o-linear-gradient(top, rgba(117,113,117,0) 0%, rgba(117,116,117,0.62) 27%, rgba(0,0,0,0.98) 100%);
    background: -ms-linear-gradient(top, rgba(117,113,117,0) 0%, rgba(117,116,117,0.62) 27%, rgba(0,0,0,0.98) 100%);
    background: linear-gradient(to bottom, rgba(117,113,117,0) 0%, rgba(117,116,117,0.62) 27%, rgba(0,0,0,0.98) 100%);
   filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#757175', endColorstr='#000000', GradientType=0 );
}

.ml-article-caption a{
    display: block;
    cursor: pointer;
    float: left;
    width: 100%;
}

.ml-article-caption a:hover{
    text-decoration: none;
    color: #fbac00 !important;
}

.ml-article-caption a:hover h3, .ml-article-caption a:hover p{
    color: #fbac00 !important;
}


.ml-article-caption h3{
